#include "export.h"

/**begin repeat
 * #TYPE = b, s, , l, ll#
 * #type = npy_byte, npy_short, npy_int, npy_long, npy_longlong#
 */

DL_EXPORT(@type@)
npy_@TYPE@abs(@type@ in)
{
    return (in >= 0) ? in : -in;
}
/**end repeat**/
